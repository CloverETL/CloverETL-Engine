<attr name="DataPolicy">Strict</attr>
</Node>
<Node enabled="enabled" fileURL="data/employees3.dat" guiHeight="25" guiName="INPUT2" guiWidth="50" guiX="18" guiY="107" id="INPUT2" type="DELIMITED_DATA_READER">
<attr name="DataPolicy">Strict</attr>
</Node>
<Node enabled="enabled" guiHeight="25" guiName="INTERSECT" guiWidth="50" guiX="195" guiY="20" id="INTERSECT" joinKey="EmployeeID" type="DATA_INTERSECTION">
<attr name="transform">${out.0.EMP_NO} = ${in.0.EmployeeID};
${out.0.FIRST_NAME} = ${in.0.FirstName};
