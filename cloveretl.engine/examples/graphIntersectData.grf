</attr>
</Node>
<Node OneRecordPerLine="true" append="false" enabled="enabled" fileURL="output/intersect_1_data.out" guiHeight="25" guiName="WRITER1" guiWidth="50" guiX="370" guiY="20" id="WRITER1" type="DELIMITED_DATA_WRITER"/>
<Node OneRecordPerLine="true" append="false" enabled="enabled" fileURL="output/intersect_1_2_data.out" guiHeight="25" guiName="WRITER2" guiWidth="50" guiX="370" guiY="107" id="WRITER2" type="DELIMITED_DATA_WRITER"/>
<Node OneRecordPerLine="true" append="false" enabled="enabled" fileURL="output/intersect_2_data.out" guiHeight="25" guiName="WRITER3" guiWidth="50" guiX="370" guiY="194" id="WRITER3" type="DELIMITED_DATA_WRITER"/>
<Edge fromNode="INPUT1:0" guiBendpoints="" id="INEDGE1" inPort="0 (INTERSECT)" metadata="InMetadata" outPort="0 (INPUT1)" toNode="INTERSECT:0"/>
<Edge fromNode="INPUT2:0" guiBendpoints="" id="INEDGE2" inPort="1 (INTERSECT)" metadata="InMetadata" outPort="0 (INPUT2)" toNode="INTERSECT:1"/>
<Edge fromNode="INTERSECT:0" guiBendpoints="" id="OUTEDGE1" inPort="0 (WRITER1)" metadata="InMetadata" outPort="0 (INTERSECT)" toNode="WRITER1:0"/>
<Edge fromNode="INTERSECT:1" guiBendpoints="" id="OUTEDGE2" inPort="0 (WRITER2)" metadata="OutMetadata" outPort="1 (INTERSECT)" toNode="WRITER2:0"/>
<Edge fromNode="INTERSECT:2" guiBendpoints="" id="OUTEDGE3" inPort="0 (WRITER3)" metadata="InMetadata" outPort="2 (INTERSECT)" toNode="WRITER3:0"/>
</Phase>
</Graph>
