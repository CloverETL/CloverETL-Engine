void unknownToken()       {
	Token t;
	
	t = getNextToken();

	if (t.kind != EOF) {
		error(t,"Syntax error on token '" + t.image + "'","Delete this token");
	} else {
		// this stops processing in case we reach EOF
		throw new EndOfFileException();
	}
}


