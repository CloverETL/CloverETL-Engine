//---------------------------------------------------------------------------------------------------------------------
@Typed
class InputPort${index} {
	
	DataRecord data
	
	<% methods.each {method -> %>
	${method["return_type"]} get${method["name"]}() {
		<% if (method["return_type"]=="String") { %>
		return data.getField(${method["index"]}).getValue().toString()
		<% } else if (method["return_type"]=="BigDecimal") { %>
		Decimal dec = (Decimal)data.getField(${method["index"]}).getValue();
		return dec.getBigDecimalOutput()
		<% } else { %>
		return data.getField(${method["index"]}).getValue()
		<% } %>
	}
	<% } %>
}

