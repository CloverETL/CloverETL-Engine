<FindBugsFilter>
     <Match>
       <Class name="~org\.jetel\.interpreter\.[^.]+Parser" />
     </Match>
     <Match>
       <Class name="~org\.jetel\.interpreter\.[^.]+Visitor" />
     </Match>
     <Match>
       <Class name="~org\.jetel\.interpreter\.Token[^.]*" />
     </Match>

     <Match>
       <Class name="org.jetel.interpreter.JavaCharStream" />
     </Match>
     <Match>
       <Class name="org.jetel.interpreter.SimpleCharStream" />
     </Match>
     <Match>
       <Class name="org.jetel.interpreter.JJTTransformLangParserState" />
     </Match>
     <Match>
       <Class name="org.jetel.interpreter.ParseException" />
     </Match>
     <Match>
       <Class name="~org\.jetel\.interpreter\.TransformLangParser[^.]*" />
     </Match>
     
     <Match>
       <Package name="~com\.google.*" />
     </Match>
     <Match>
       <Package name="~org\.apache.*" />
     </Match>
     <Match>
       <Package name="~org\.slf4j.*" />
     </Match>
     <Match>
       <Package name="~org\.hsqldb.*" />
     </Match>

     <Match>
     	<Bug code="PZLA,UwF,Dm,EI2" />
     </Match>
     <Match>
     	<Bug pattern="EI_EXPOSE_REP" />
     	<Bug pattern="NM_CONFUSING" />
     	<Bug pattern="NP_GUARANTEED_DEREF_ON_EXCEPTION_PATH" />
     	<Bug pattern="OBL_UNSATISFIED_OBLIGATION_EXCEPTION_EDGE" />
     	<Bug pattern="ODR_OPEN_DATABASE_RESOURCE_EXCEPTION_PATH" />
     	<Bug pattern="REC_CATCH_EXCEPTION" />
     	<Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE" />
     	<Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NULL_VALUE" />
     	<Bug pattern="SIC_INNER_SHOULD_BE_STATIC_ANON" />
     </Match>

</FindBugsFilter>