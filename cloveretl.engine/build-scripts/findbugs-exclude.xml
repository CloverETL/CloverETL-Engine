<FindBugsFilter>
     <Match>
       <Class name="~org\.jetel\.interpreter\.[^.]+Parser" />
     </Match>
     <Match>
       <Class name="~org\.jetel\.interpreter\.[^.]+Visitor" />
     </Match>
     <Match>
       <Class name="~org\.jetel\.interpreter\.Token[^.]*" />
     </Match>

     <Match>
       <Class name="org.jetel.interpreter.JavaCharStream" />
     </Match>
     <Match>
       <Class name="org.jetel.interpreter.SimpleCharStream" />
     </Match>
     <Match>
       <Class name="org.jetel.interpreter.JJTTransformLangParserState" />
     </Match>
     <Match>
       <Class name="org.jetel.interpreter.ParseException" />
     </Match>
     <Match>
       <Class name="~org\.jetel\.interpreter\.TransformLangParser[^.]*" />
     </Match>
     
     <!-- Test if it works for check run by jenkins -->
     <Match>
     	<Class name="org.jetel.component.XMLExtract" />
     	<Method name="checkConfig" />
     	<Bug pattern="RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE" />
     </Match>

     <Match>
     	<Bug code="PZLA" />
     </Match>
     <Match>
     	<Bug code="UwF" />
     </Match>
     <Match>
     	<Bug code="Dm" />
     </Match>

</FindBugsFilter>